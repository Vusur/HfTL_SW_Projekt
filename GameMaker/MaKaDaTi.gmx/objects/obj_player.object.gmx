          <argument>
            <kind>1</kind>
            <string>///behavior of the player
scr_getInputs();

//draw the direction which the player faces
if((key_right - key_left) != 0) image_xscale = key_right - key_left;
var_player_direction = image_xscale;

//fall
if(vsp &lt; 10) vsp += grav;

//jump
if(place_meeting(x, y+1, obj_stoneWall)){
    if(key_up) vsp = -jumpspeed;
}

//move
hsp = movespeed * (key_right - key_left);

//###################################
//### collision with solid Blocks ###
//###################################

//vertical
if(place_meeting(x, y+vsp, obj_stoneWall)){
    while(!place_meeting(x, y+sign(vsp), obj_stoneWall)){
        y += sign(vsp);
    }
    vsp = 0;   
}

//horizontal
if(place_meeting(x+hsp, y, obj_stoneWall)){
    while(!place_meeting(x+sign(hsp), y, obj_stoneWall)){
        x += sign(hsp);
    }
    hsp = 0;
}


//compute new position
x += hsp;
y += vsp;

if(hsp != 0 &amp;&amp; place_meeting(x, y+1, obj_stoneWall) ){
    sprite_index = spr_player_run;
} else {
    sprite_index = spr_player_idle;
}

//#############
//### DEBUG ###
//#############
scr_debug();

    



