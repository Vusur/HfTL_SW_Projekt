            <kind>1</kind>
            <string>/// Movement

hsp = dir * movespeed;
vsp += grav;

//###################################
//### collision with solid Blocks ###
//###################################

//vertical
if(place_meeting(x, y+vsp, obj_stoneWall)){
    while(!place_meeting(x, y+sign(vsp), obj_stoneWall)){
        y += sign(vsp);
    }
    vsp = 0;   
}

//horizontal
if(place_meeting(x+hsp, y, obj_stoneWall)){
    while(!place_meeting(x+sign(hsp), y, obj_stoneWall)){
        x += sign(hsp);
    }
    hsp = 0;
    dir *= -1;
}

//compute new position
x += hsp;
y += vsp;

//Enemy Collision
if (place_meeting(x,y,obj_player)){
    if (obj_player.y &lt; y-50)
    {
        with (obj_player) vsp = -jumpspeed;
        instance_destroy();
    }else{
        room_restart();
    }
}

//if (hsp == 0) sprite_index = spr_pumpkin01_waiting;
if (hsp == 0) image_xscale *= -1;
</string>
