            <string>///Jump-Kill
//is paused?
if(global.pause){
    audio_stop_sound(snd_player_gameover001);
    exit;
}

//Enemy Collision
if (place_meeting(x,y,obj_player) &amp;&amp; !obj_player.ishitted){
    if (obj_player.y &lt; y)
    {
        with (obj_player) vsp = -jumpspeed*1.5;
        if(!ishitted){
            var_health-=100;
            ishitted=true;
            alarm[3]=room_speed/2;
            effect_create_above(ef_firework,obj_player.x,obj_player.y,64,c_white);
            obj_player.x=200;
            obj_player.y=400;
            effect_create_above(ef_firework,obj_player.x,obj_player.y,64,c_white);
        }
        var i = choose(snd_say_grr001, say_au001);
        audio_play_sound(i, 10, false);
    }else{
        with(obj_player){
            ishitted = true;
            vsp = -jumpspeed/1.2;
            moveable = false;
            sprite_index = spr_player_idle;
            image_angle = choose(-90, 90);
            alarm[2] = room_speed * 2;
            audio_play_sound(snd_player_gameover001, 10, false);
        }
    }
}

//Health &lt; 1
if (var_health &lt; 1) instance_destroy();
