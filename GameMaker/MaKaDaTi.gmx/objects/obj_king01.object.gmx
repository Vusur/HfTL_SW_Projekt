            <string>///Schießen
event_inherited();

if(global.pause){
    exit;
}

var xdis = sqrt(sqr(obj_player.x - x)) 
var ydis = sqrt(sqr(obj_player.y - y))

if(xdis &lt; 1500 &amp;&amp; ydis &lt; 300){   //Spieler befindet sich im Radius.
    if(shoot != true &amp;&amp; !wait){
        audio_play_sound(snd_evil_hehehe001, 1, false);
    }
    if(!wait){  //Kürbis kann aktiviert werden
        shoot = true;   //er schießt
        alarm[1] = room_speed*2;  //Solange er schießen kann, wird resettet
    }
} else {    //Spieler ist außerhalb des Radius
    if(shoot &amp;&amp; !wait){ //er schießt und kann aktiviert werden
        shoot = false;   //hör auf zu schießen
        wait = true;    //kann nicht aktiviert werden
        alarm[2] = room_speed;
        var_phase = 2; 
        
    }
    
}
