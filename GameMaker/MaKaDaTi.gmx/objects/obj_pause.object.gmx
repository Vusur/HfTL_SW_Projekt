          <argument>
            <kind>1</kind>
            <string>scr_getInputs(true);

if(key_pause){
    if(global.pause){
        global.pause = false;
        entry = 0;
    } else {
        global.pause = true;
        entry = 1;
    }
}

if(!global.pause) exit;

//choose entry
if(key_down &amp;&amp; entry  &lt; 3){
    entry += 1;
} else if (key_up &amp;&amp; entry &gt; 1){
    entry -= 1;
}

//compute Entry

//### Continue
if(entry == 1 &amp;&amp; key_enter) {
    global.pause = false;
    entry = 0;
}
//### Restart
if(entry == 2 &amp;&amp; key_enter){
    global.pause = false;
    entry = 0;
    room_restart();
}
    
//### Save and Exit
if(entry == 3 &amp;&amp; key_enter){
    global.pause = false;
    entry = 0;
    
    //save the Game
    if(file_exists(global.save)) file_delete(global.save);
    var savefile = file_text_open_write(global.save);
    var rm = room;
    file_text_write_real(savefile, rm);
    file_text_close(savefile);
    
    room_goto(rm_000menu);
}
</string>
